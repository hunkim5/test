<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.airpremia.eosh.ca.repository.SubscribeCaRepository">
    <select id="selectDetail" resultType="SubscribeCaEntity">
        SELECT
        t_mast.subscribe_ca_uuid
        ,t_mast.insert_user_id
        ,t_mast.insert_dtm
        ,t_mast.insert_ip
        ,t_mast.update_user_id
        ,t_mast.update_dtm
        ,t_mast.update_ip
        FROM
        subscribe_ca t_mast
        <where>
            t_mast.subscribe_ca_uuid = #{subscribeCaUuid}
        </where>
    </select>
    <insert id="insertSubscribeCa">
        INSERT INTO subscribe_ca (
            subscribe_ca_uuid
            ,insert_user_id
            ,insert_dtm
            ,insert_ip
            ,update_user_id
            ,update_dtm
            ,update_ip
            )
        VALUES (
            #{subscribeCaUuid}
            ,#{insertUserId}
            ,#{insertDtm}
            ,#{insertIp}
            ,#{updateUserId}
            ,#{updateDtm}
            ,#{updateIp}
            )
    </insert>
    <update id="updateSubscribeCa">
        UPDATE subscribe_ca
        SET
        update_user_id = #{updateUserId}
        ,update_dtm = #{updateDtm}
        ,update_ip = #{updateIp}
        <where>
            and subscribe_ca_uuid = #{subscribeCaUuid}
        </where>
    </update>
</mapper>
